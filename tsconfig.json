{
  "compilerOptions": {
      "declaration": true, /* 生成相关的 '.d.ts' 文件。 */
      "declarationDir": "./dist/types", /* '.d.ts' 文件输出目录 */
      "jsx": "preserve",
      "types": [
        "vitest/globals"
      ],
  },
  "include": [
      "./**/*.*",
      "./shims-vue.d.ts",
      "docs/.vuepress/config.ts",
      "docs/.vitepress/theme/index.ts",
  ],
  "exclude": [
      "node_modules"
  ],
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": "true"
}
